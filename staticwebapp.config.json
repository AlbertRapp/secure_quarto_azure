{
    "routes": [
      {
        "route": "/*",
        "allowedRoles": ["authenticated"]
      }
    ],
    "responseOverrides": {
      "401": {
        "statusCode": 302,
        "redirect": "/.auth/login/github"
      }
    },
    "auth": {
        "identityProviders": {
            "github": {
                "registration": {
                    "clientIdSettingName": "GITHUB_CLIENT_ID",
                    "clientSecretSettingName": "GITHUB_CLIENT_SECRET_APP_SETTING_NAME"
                }
            }
        }
    }
}